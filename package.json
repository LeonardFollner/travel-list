{
  "name": "travel-list",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "node-sass": "^3.8.0",
    "raw-loader": "^0.5.1",
    "react-scripts": "0.9.5",
    "sass-loader": "^4.0.2",
    "stylelint-config-property-sort-order-smacss": "^1.0.2",
    "stylelint-config-standard": "^15.0.1",
    "stylelint-webpack-plugin": "^0.5.1"
  },
  "dependencies": {
    "@neos-project/eslint-config-neos": "^1.1.1",
    "classnames": "^2.2.5",
    "font-awesome": "^4.6.3",
    "immutable": "^3.8.1",
    "moment": "^2.14.1",
    "moment-timezone": "^0.5.5",
    "plow-js": "^1.2.0",
    "react": "^15.4.2",
    "react-app-rewired": "^0.6.5",
    "react-dom": "^15.4.2",
    "react-router": "^2.8.1",
    "sass-lint": "^1.10.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "eject": "react-scripts eject",
    "storybook": "STORYBOOK_MODE=1 start-storybook -p 9001 -c .storybook",
    "test": "react-scripts test --env=jsdom",
    "lint:sass": "sass-lint -c ./.sass-lint.yml -v"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "babel",
      "react",
      "jsx-a11y",
      "import"
    ],
    "extends": [
      "xo-space/esnext",
      "xo-react/space"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "babel/object-shorthand": 0,
      "no-unreachable": 0,
      "react/jsx-first-prop-new-line": 0,
      "react/jsx-closing-bracket-location": 0,
      "no-useless-constructor": 0,
      "no-else-return": 0,
      "no-prototype-builtins": 0,
      "babel/new-cap": 0,
      "block-scoped-var": 0,
      "max-statements-per-line": 0,
      "jsx-a11y/label-has-for": 0,
      "jsx-a11y/mouse-events-have-key-events": 0,
      "no-case-declarations": 0,
      "react/no-danger": 0,
      "no-negated-condition": 0,
      "no-redeclare": 0,
      "no-unmodified-loop-condition": 0,
      "array-callback-return": 0,
      "camelcase": 0,
      "jsx-a11y/img-has-alt": 0,
      "no-debugger": 1,
      "no-warning-comments": 0,
      "no-unused-vars": 1,
      "react/jsx-boolean-value": 0,
      "react/jsx-filename-extension": 0,
      "react/jsx-indent-props": [
        2,
        4
      ],
      "react/jsx-indent": [
        2,
        4
      ],
      "indent": [
        2,
        4,
        {
          "SwitchCase": 1
        }
      ],
      "jsx-a11y/aria-props": 2,
      "jsx-a11y/aria-proptypes": 2,
      "jsx-a11y/aria-role": 2,
      "jsx-a11y/aria-unsupported-elements": 2,
      "jsx-a11y/img-redundant-alt": 2,
      "jsx-a11y/no-access-key": 2,
      "jsx-a11y/onclick-has-role": 1,
      "jsx-a11y/role-has-required-aria-props": 2,
      "jsx-a11y/role-supports-aria-props": 2,
      "jsx-a11y/tabindex-no-positive": 1
    },
    "globals": {
      "it": true,
      "expect": true
    }
  }
}
